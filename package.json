    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@gmod/cram": "^1.5.3",
    "@gmod/indexedfasta": "^1.0.13",
    "axios": "^0.19.0",
